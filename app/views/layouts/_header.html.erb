<header>
  <nav class="navbar navbar-default navbar-fixed-top">
    <div class="container">
      <div class="navbar-header">
        <!-- The mobile navbar-toggle button can be safely removed since you do not need it in a non-responsive implementation -->
        <%= link_to mypage_path(current_user.id), class: "navbar-brand" do %>
          <img src="/images/dogingLogo2.png" width="130" height="40">
        <% end %>
      </div>
      <!-- Note that the .navbar-collapse and .collapse classes have been removed from the #navbar -->
      <div id="navbar">
        <ul class="nav navbar-nav navbar-right">
          <li>
            <%= link_to mypage_path(current_user.id) do %>
              <font size="5"><i class="fa fa-home fa-lg"></font></i>
            <% end %>
          </li>
          <li><a href="/requests/new"><font size="5"><i class="fa fa-pencil-square-o"></i></font></a></li>
          <li><a href="/requests/"><font size="5"><i class="fa fa-search"></i></font></a></li>
          <li><a href="/dogs/new"><font size="5"><i class="fa fa-cog"></i></font></a></li>
          <li>
            <%= link_to destroy_user_session_path, method: "delete" do %>
              <font size="5"><i class="fa fa-sign-out"></i>
            <% end %>
          </li>
        </ul>
      </div><!--/.nav-collapse -->
    </div>
  </nav>
</header>
